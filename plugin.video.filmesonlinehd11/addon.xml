<?xml version="0.0.1" encoding="UTF-8" standalone="yes"?>
<addon id="plugin.video.filmesonlinehd11" name=".FilmesHD11" version="1.1.2" provider-name="AddonReneSilva">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
		<import addon="script.module.urlresolver" version="2.10.8"/>
		<import addon="script.extendedinfo" version="3.0.0"/>
		<import addon="script.module.videohosts" version="1.0.0"/>
        <import addon="script.module.beautifulsoup4" version="4.3.2"/>
        <import addon="script.module.requests" version="2.9.1"/>
  </requires>
	
  <extension point="xbmc.python.pluginsource" library="addon.py">
    <provides>video</provides>
  </extension>
	
  <extension point="xbmc.addon.metadata">
    <language>en</language>
    <platform>all</platform>
    <summary lang="en">Filmes e Series Online</summary>
    <description lang="en">
			No addon FilmesOnlineHD11 você assiste filmes separados por categorias, lançamentos,filmes HD, etc.
			Você também pode ver as informações ou assistir ao trailer do filme no submenu do addon (Botão direito do mouse ou tecla "C").
			Atencao: Este addon necessita do script URLRESOLVER na versão 2.10.8 ou superior
		</description>
  </extension>
</addon>
