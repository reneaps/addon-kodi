<addon id="plugin.video.teutorrent" name="TeuTorrent" version="1.0.0" provider-name="AddonReneSilva">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
		<import addon="script.module.urlresolver" version="2.10.8" />
		<import addon="script.extendedinfo" version="3.0.0"/>
  </requires>
	
  <extension point="xbmc.python.pluginsource" library="addon.py">
    <provides>video</provides>
  </extension>
	
  <extension point="xbmc.addon.metadata">
    <language>en</language>
    <platform>all</platform>
    <summary lang="en">Filmes e Series Online</summary>
    <description lang="en">
			No addon TeuTorrent você assiste filmes separados por categorias, lançamentos,filmes HD, etc. \
			Você também pode ver as informações ou assistir ao trailer do filme no submenu do addon (Botão direito do mouse ou tecla "C"). \
			Atencao: Este addon necessita do script URLRESOLVER na versão 2.10.8 ou superior
		</description>
  </extension>
</addon>
